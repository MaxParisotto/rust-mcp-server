{
  "name": "rust-mcp-server",
  "version": "1.0.0",
  "description": "A server for analyzing Rust code via the Model Context Protocol (MCP)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "rust-mcp-server": "./dist/index.js"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "rust",
    "analyzer",
    "mcp",
    "lsp"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "cors": "^2.8.5",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "20.17.24",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.4",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.3",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "dev": "tsx src/index.js",
    "build": "tsc",
    "build:all": "tsc && cd rust-bridge && cargo build --release",
    "start": "node dist/index.js",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --coverage",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'"
  }
}
