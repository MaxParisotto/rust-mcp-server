{
  "name": "rust-mcp-server",
  "version": "1.0.0",
  "description": "A server for analyzing Rust code via the Model Context Protocol (MCP)",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "rust-mcp-server": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "build:all": "tsc && cd rust-bridge && cargo build --release",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "MIT",
  "keywords": [
    "rust",
    "analyzer",
    "mcp",
    "lsp"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "uuid": "^9.0.0",
    "ws": "^8.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.3",
    "@types/uuid": "^9.0.2",
    "@types/ws": "^8.5.4",
    "tsx": "^3.12.10",
    "typescript": "^5.1.3"
  }
}
